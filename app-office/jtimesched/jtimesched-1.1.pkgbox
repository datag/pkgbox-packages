# pkgbox: jTimeSched

PKGBOX_API=1

MY_PN=jTimeSched
MY_P=$MY_PN-$PV

DESCRIPTION="jTimeSched is a simple and lightweight time tracking tool"
HOMEPAGE="http://kbase.dominik-geyer.de/apps:jtimesched"
SRC_URI="http://www.dominik-geyer.de/files/$MY_PN/$MY_P.zip"

IUSE=""

S=${WORKDIR}/${MY_P}

src_unpack() {
	# package doesn't have $P as first path component
	pkgbox_unpack "$A" "$S"
}

